{
  "name": "@timberstack/echoes",
  "version": "0.0.0",
  "description": "Lightweight reactivity library for vanilla JS",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "vitest",
    "fmt": "biome format --write",
    "lint": "biome lint",
    "cs": "changeset",
    "version": "changeset version",
    "version:test": "changeset version --snapshot charmander",
    "release": "pnpm run minify && changeset publish",
    "release:test": "changeset publish --no-git-tag --tag charmander",
    "minify": "node ./scripts/minify.js"
  },
  "keywords": ["reactivity"],
  "author": "Álvaro Palma Mancheño",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timberstack/echoes.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.28.1",
    "esbuild": "^0.25.2",
    "vitest": "^3.0.9"
  },
  "files": ["src/", "dist/", "README.md"],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879"
}
